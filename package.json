{
    "name": "bgg-games-ranks-cacher",
    "version": "0.1.0",
    "description": "bgg games ranks cacher",
    "main": "index.js",
    "engines": {
        "node": ">=18.0",
        "npm": ">=8.0"
    },
    "scripts": {
        "test": "echo \"Tests are not implemented yet\"",
        "build": "rm -rf dist && tsc",
        "dev": "npm run build && GOOGLE_APPLICATION_CREDENTIALS=key-file.json npx @google-cloud/functions-framework --target=main --source=dist",
        "deploy": "npm run build && cp package*.json dist && gcloud functions deploy bgg-games-ranks-cacher --gen2 --trigger-http --runtime nodejs20 --entry-point=main --no-allow-unauthenticated --region=europe-central2 --source=dist --update-build-env-vars=\"GOOGLE_NODE_RUN_SCRIPTS=\" --project zinovik-project --service-account=bgg-games-ranks-cacher@zinovik-project.iam.gserviceaccount.com"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "3.4.2",
        "@google-cloud/storage": "7.12.1",
        "google-auth-library": "9.14.1"
    },
    "devDependencies": {
        "@types/jest": "29.5.12",
        "@types/node": "22.5.4",
        "@typescript-eslint/eslint-plugin": "8.5.0",
        "@typescript-eslint/parser": "8.5.0",
        "eslint": "9.10.0",
        "jest": "29.7.0",
        "ts-jest": "29.2.5",
        "typescript": "5.6.2"
    },
