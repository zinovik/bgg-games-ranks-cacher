{
    "name": "bgg-games-ranks-cacher",
    "version": "0.1.0",
    "description": "bgg games ranks cacher",
    "main": "index.js",
    "engines": {
        "node": ">=18.0",
        "npm": ">=8.0"
    },
    "scripts": {
        "test": "echo \"Tests are not implemented yet\"",
        "build": "rm -rf dist && tsc",
        "dev": "npm run build && GOOGLE_APPLICATION_CREDENTIALS=key-file.json npx @google-cloud/functions-framework --target=main --source=dist",
        "deploy": "npm run build && cp package*.json dist && gcloud functions deploy bgg-games-ranks-cacher --gen2 --trigger-http --runtime nodejs20 --entry-point=main --region=us-central1 --source=dist --update-build-env-vars=\"GOOGLE_NODE_RUN_SCRIPTS=\" --project zinovik-project",
        "service:account": "gcloud iam service-accounts keys create key-file.json --iam-account=zinovik-project@appspot.gserviceaccount.com",
        "describe": "gcloud functions describe bgg-games-ranks-cacher --region=us-central1"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "3.3.0",
        "@google-cloud/storage": "7.7.0",
        "google-auth-library": "9.4.1"
    },
    "devDependencies": {
        "@types/jest": "29.0.1",
        "jest": "29.0.3",
        "ts-jest": "29.0.0",
        "typescript": "5.3.3"
    },
    "author": "Max Zinovik",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zinovik/bgg-games-ranks-cacher.git"
    },
    "bugs": {
        "url": "https://github.com/zinovik/bgg-games-ranks-cacher/issues"
    },
    "homepage": "https://github.com/zinovik/bgg-games-ranks-cacher#readme"
}
